#include<iostream>
using namespace std;
int main()
{
    int num,ans=0;
    for (int i = 1; i <= 5; i++)
    {
        for (int j = 1; j <= 5; j++)
        {
            cin >> num;
            if(num==1){
                if(i>3)ans+=i-3;
                else if(i<3) ans+=3-i;
                if(j>3)ans+=j-3;
                else if(j<3) ans+=3-j;
            }
        }
        
    }
    cout << ans << endl;
    
    
 return 0;
}
